<%- include('partials/header')%>


<h1>Home</h1>


<p> <%=HomeContent%></p>
<%post.forEach((item) => {%>
  <h1><%=item.title%></h1>


  <%  if (item.body.length > 100) { %>
      <p>  <%=item.body.slice(0,100) +'...' %>
      <a href="/posts/<%=item.title%>">Read more</a>
      </p>


    <%}%>


<%});%>






